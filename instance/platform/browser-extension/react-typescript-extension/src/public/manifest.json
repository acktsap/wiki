{
  "manifest_version": 2,
  "name": "react-browser-extension",
  "description": "test description",
  "version": "0.1.0",
  "icons": {
    "32": "icons/beasts-30.png",
    "48": "icons/beasts-60.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.mozilla.org/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icons/beasts-30.png",
    "default_title": "browser-extension",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "option.html"
  },
  "web_accessible_resources": [
    "img/frog.jpg",
    "img/snake.jpg",
    "img/turtle.jpg"
  ]
}
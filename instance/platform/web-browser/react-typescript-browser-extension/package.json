{
  "name": "react-typescript-browser-extension",
  "version": "0.1.0",
  "description": "react-typescript-browser-extension",
  "scripts": {
    "start": "yarn run start:firefox",
    "start:firefox": "webpack -w --env target=firefox --mode=production",
    "start:chrome": "webpack -w --env target=chrome --mode=production",
    "lint": "eslint ./src --fix",
    "build": "webpack --mode=production",
    "package": "yarn run build && web-ext build -s build --overwrite-dest -a dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acktsap/xxx.git"
  },
  "keywords": [
    "extension"
  ],
  "author": "Taeik lim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acktsap/xxx/issues"
  },
  "homepage": "https://github.com/acktsap/xxx#readme",
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "eslint": "^7.25.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "sass": "^1.32.12",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.4",
    "web-ext": "^6.1.0",
    "web-ext-plugin": "^1.1.0",
    "webpack": "^5.36.0",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}

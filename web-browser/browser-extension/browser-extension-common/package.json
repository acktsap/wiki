{
  "name": "browser-extension",
  "version": "0.1.0",
  "description": "browser-extension",
  "scripts": {
    "start": "yarn run start:firefox",
    "start:firefox": "web-ext run -s ./src --browser-console --start-url https://duckduckgo.com --target firefox-desktop",
    "start:chrome": "web-ext run -s ./src --browser-console --start-url https://duckduckgo.com --target chromium",
    "build": "web-ext build -s ./src --overwrite-dest -a dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acktsap/xxx.git"
  },
  "keywords": [
    "extension"
  ],
  "author": "Taeik lim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acktsap/xxx/issues"
  },
  "homepage": "https://github.com/acktsap/xxx#readme",
  "devDependencies": {
    "web-ext": "^6.1.0"
  }
}
